<%= simple_form_for(@job) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :completed_at %>
    <%= f.input :description %>
    <%= f.input :title %>
    <%= f.input :organization %>
    <%= f.input :user_id %>
    <%= f.input :location %>
    <%= f.input :skills %>
    <%= f.input :experience %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
